{{!< default}}

{{> cover this}}

<section class="wrapper" tabindex="0">
  <div class="wrapper-container">
    {{> custom/featured this}}

    <section class="post-list">
      {{#foreach posts}}
        {{> post/list this}}
      {{/foreach}}
    </section>

    {{#if pagination}}
      {{pagination}}
    {{/if}}

    {{> footer this}}
  </div>
</section>

<script type="text/javascript">
  if (typeof MathJax != 'undefined') {
    MathJax.Hub.Queue(["Typeset",MathJax.Hub]);
  }
</script>
